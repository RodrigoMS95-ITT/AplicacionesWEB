@model PS01.Models.City

@{
    ViewBag.Title = "Details";
}
<div class="masthead space">
    <div class="masthead-content">
        <div class="container display2 space">
            <h2>Detalles</h2>

            <div>
                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        Ciudad
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Name)
                    </dd>

                    <dt>
                        Estado
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.State)
                    </dd>

                </dl>
            </div>
            <p>
                @Html.ActionLink("Editar", "Edit", new { id = Model.id }, new { @class = "btn btn-default btn-info" }) |
                @Html.ActionLink("Regresar", "Index", new { area = "" }, new { @class = "btn btn-default btn-info" })
            </p>
        </div>
    </div>
</div>